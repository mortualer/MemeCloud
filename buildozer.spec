[app]
title=MemeCloud
package.name=memecloud
package.domain=org.example
source.dir=.
source.include_exts=py,png,jpg,jpeg,mp3,kv,json
version=1.0
requirements=python3,kivy
orientation=portrait
fullscreen=0
android.api=33
android.minapi=21
android.archs=arm64-v8a,armeabi-v7a
icon.filename=memeicon.jpg
presplash.filename=
android.permissions=WRITE_EXTERNAL_STORAGE,READ_EXTERNAL_STORAGE

[buildozer]
log_level=2
warn_on_root=1

